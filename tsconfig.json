{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "noImplicitReturns": true,
    "outDir": "build",
    "sourceMap": true,
    "jsx": "react-jsx",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "compileOnSave": true,
  "include": [
    "Components",
    "App.tsx"
  ]
}
